# Configuración del servidor
PORT=3001
SERVER=localhost:3001

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# ElevenLabs Configuration (Opcional - usa Google TTS si no se configura)
USE_ELEVENLABS=true
ELEVENLABS_VOICE_ID=tu_voice_id_de_elevenlabs

# Configuración de servicios
USE_REALTIME_API=true

# ========================================
# CONFIGURACIÓN PARA LLAMADAS OUTBOUND
# ========================================

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_FROM_NUMBER=+1234567890

# Rocketbot SaturnStudio Integration
ROCKETBOT_WEBHOOK_URL=https://studio.rocketbot.com/webhook/your_webhook_id_here

# Transfer Configuration
TRANSFER_NUMBER=+1234567890 